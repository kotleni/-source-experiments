/*source "C:\\dev\\main\\game\\sdktools\\maya\\7.0\\modules\\tf\\scripts\\tfBodyGroupExport.mel" ;tfBodyGroupExport "sentry2_" "sentry2Info", "C:/dev/main/game/tf/../../content/tf/models/buildables/sentry1/parts/dmx/" ;*/// jake nicholson, oct 2006// goal:// use:  //is specific to sentry2 right now...global proc tfBodyGroupExport ( string $baseName, string $infoNode, string $baseFilePath ){string $allModels[] = `ls -sl` ;//"C:/dev/main/game/tf/../../content/tf/models/buildables/sentry1/parts/dmx/" ;string $fullExportString ; string $thisDMXFileName ;int $s ;  int $i ;int $j ;for ( $i = 0 ; $i < `size $allModels` ; $i++ ){	// first parent all models to world	for ( $j = 0 ; $j < `size $allModels` ; $j++ )	{		catch ( `parent -w $allModels[$j]` ) ;	}	//get the filename we'll use for export. assuming "BGroup_" name convention for groups, so strip that out	$s = `size $allModels[$i]` ;	$thisDMXFileName = ( $baseName + `substring $allModels[$i] 8  $s` + ".dmx" ) ;	//print $thisDMXFileName ;	$fullExportString = ( $baseFilePath + $thisDMXFileName ) ;		// make $i a child of infoNode, export model dmx	parent $allModels[$i] $infoNode ;	//vsDmxIO -sl -export -et m  -filename $fullExportString sentry2Info;		vsDmxIO -sl -export -et m  -filename $fullExportString $infoNode ;	print ("\nExported: " + $fullExportString + "\n" ) ;}//cleanup.  make all groups child of infoNode again.	for ( $i = 0 ; $i < `size $allModels` ; $i++ )		{			catch ( `parent $allModels[$i] $infoNode` ) ;		}}  // Fin