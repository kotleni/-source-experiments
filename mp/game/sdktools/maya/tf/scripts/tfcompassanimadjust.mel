global proc tfCompassAnimAdjust( string $inDir, string $outDir ){//currently works on all objects selected.//assumes playback range is set to the entire loop.//assumes other crap i'm surestring $sel[] = `ls -sl` ;float $scalePivot ;float $tEnd = `playbackOptions -q -maxTime` ;float $tStart = `playbackOptions -q -minTime` ;string $tBound = ( $tStart + ":" + $tEnd ) ;//print $tBound ;float $temp ;//string $inDir = "N" ;//string $outDir = "NW" ;if ($inDir == "N"){ 	if ( ( $outDir == "NW" ) || ( $outDir == "NE" ) )	{		for ($i = 0; $i < `size ($sel)`; $i++)		{			float $absDelta ;			//get the tz value @ frame 0			$scalePivot = ( `getAttr -t 0 ( $sel[$i] + ".translateZ" )` ) ;			//print $scalePivot ;			//scale the tz component			scaleKey -t ":" -vs 0.707 -vp $scalePivot -attribute tz $sel[$i];			//get the absDelta of the scaled tz component			$temp = ( `getAttr -t $tEnd ( $sel[$i] + ".translateZ" )` ) ; 			$absDelta = ( $temp - $scalePivot ) ;			//print $absDelta ;			//how to handle adjusting the tx component:			//for now, add the ( absDelta / time ) to each keyframe's value						//get the key info for tx component			float $keyTimes[] = ( `keyframe -t $tBound -attribute translateX -query -tc $sel[$i]` ) ;			float $keyValues[] = ( `keyframe -t $tBound -attribute translateX -query -vc $sel[$i]` ) ;			print $keyTimes ;			print $keyValues ;			//add the appropriate value delta to each tx key			for ($j = 0; $j < `size($keyTimes)`; $j++)			{					float $thisScalar = ( $keyTimes[$j] / $tEnd ) ;				print $thisScalar ;				float $thisDelta = ( $absDelta * $thisScalar ) ;				float $thisValue ;				if ( $outDir == "NW" )				{					$thisValue = ( $keyValues[$j] + $thisDelta ) ;				}				else					$thisValue = ( $keyValues[$j] - $thisDelta ) ;				keyframe -t $keyTimes[$j] -attribute translateX -a -vc $thisValue $sel[$i] ;			}		}		}}}