global proc vstHideUnnecessary(string $stuff[])
{
	string $DLAM = "dontLookAtMe";
	if (!`objExists $DLAM`)	
	{
		group -empty -n $DLAM;
		hide $DLAM;
		
		createDisplayLayer -name "OFF" -number 1 -empty;
		setAttr OFF.displayType 2;
		editDisplayLayerMembers OFF $DLAM;
	}
	if (size($stuff))
	{
		parent -a $stuff $DLAM;
	}
}
