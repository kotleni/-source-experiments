global proc brHideUnnecessary (string $stuff[])
{
	$DLAM = "dontLookAtMe";
	if (!`objExists $DLAM`)	
	{
		group -empty -n $DLAM;
		hide $DLAM;
		
		createDisplayLayer -name "OFF" -number 1 -empty;
		layerEditorLayerButtonTypeChange OFF;
		layerEditorLayerButtonTypeChange OFF;
		select -r $DLAM ;
		layerEditorAddObjects OFF;
	}
	if (`size ($stuff)` > 0)
		parent -a $stuff $DLAM;

	

}
