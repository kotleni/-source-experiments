//cam crap/*string $thisCamera[] = `camera -centerOfInterest 5 -n`;print $thisCamera[0];string $camN = `cameraMakeNode 2 $thisCamera[0]`;print $camN;connectAttr -f UpperbodyControl.translateZ camera1.translateZ;connectAttr -f UpperbodyControl.translateX camera1.translateX;camera -centerOfInterest 5 -focalLength 35 -lensSqueezeRatio 1 -cameraScale 1 -horizontalFilmAperture 1.4173 -horizontalFilmOffset 0 -verticalFilmAperture 0.9449 -verticalFilmOffset 0 -filmFit Horizontal -overscan 1 -motionBlur 0 -shutterAngle 144 -nearClipPlane 0.01 -farClipPlane 1000 -orthographic 0 -orthographicWidth 30; objectMoveCommand; cameraMakeNode 2 "";*/global proc tfRigMedic_2ndryCoatAdjust (){//create the control, constrain to the upperbodystring $thisControl[] = ( `circle -c 0 0 0 -nr 0 1 0 -sw 360 -r 15 -d 3 -ut 0 -tol 0.00155 -s 8 -ch 1 -n "nurbsCircle1"` ) ; pointConstraint -offset 0 0 0 -weight 1 "UpperbodyControl" $thisControl[0] ;string $thisAttr = ( $thisControl[0] + ".rotateOrder" );setAttr $thisAttr 3;//make the hack hierarchystring $thisGrp = `group -n "coatHackGroupL" -em` ;string $tempCns[] = `pointConstraint -offset 0 0 0 -weight 1 "Ref:prp_coat_front_1_LControl" $thisGrp` ;delete $tempCns[0] ;parent $thisGrp "nurbsCircle1" ;parentConstraint -mo -weight 1 -sr x -sr y -sr z "UpperbodyControl" $thisGrp ;parent "Ref:prp_coat_front_1_LControlZeroGroup" $thisGrp ;string $thisGrp = `group -n "coatHackGroupR" -em` ;string $tempCns[] = `pointConstraint -offset 0 0 0 -weight 1 "Ref:prp_coat_front_1_RControl" $thisGrp` ;delete $tempCns[0] ;parent $thisGrp "nurbsCircle1" ;parentConstraint -mo -weight 1 -sr x -sr y -sr z "UpperbodyControl" $thisGrp ;parent "Ref:prp_coat_front_1_RControlZeroGroup" $thisGrp ;string $thisGrp = `group -n "coatHackGroupBackL" -em` ;string $tempCns[] = `pointConstraint -offset 0 0 0 -weight 1 "Ref:prp_coat_back_0_LControl" $thisGrp` ;delete $tempCns[0] ;parent $thisGrp "nurbsCircle1" ;parentConstraint -mo -weight 1 -sr x -sr y -sr z "UpperbodyControl" $thisGrp ;parent "Ref:prp_coat_back_0_LControlZeroGroup" $thisGrp ;string $thisGrp = `group -n "coatHackGroupBackMid" -em` ;string $tempCns[] = `pointConstraint -offset 0 0 0 -weight 1 "Ref:prp_coat_back_0_MControl" $thisGrp` ;delete $tempCns[0] ;parent $thisGrp "nurbsCircle1" ;parentConstraint -mo -weight 1 -sr x -sr y -sr z "UpperbodyControl" $thisGrp ;parent "Ref:prp_coat_back_0_MControlZeroGroup" $thisGrp ;string $thisGrp = `group -n "coatHackGroupBackR" -em` ;string $tempCns[] = `pointConstraint -offset 0 0 0 -weight 1 "Ref:prp_coat_back_0_RControl" $thisGrp` ;delete $tempCns[0] ;parent $thisGrp "nurbsCircle1" ;parentConstraint -mo -weight 1 -sr x -sr y -sr z "UpperbodyControl" $thisGrp ;parent "Ref:prp_coat_back_0_RControlZeroGroup" $thisGrp ;//delete the existing point constraints on the coat controls' groupsselect -r Ref:prp_coat_front_1_LControlZeroGroup ;select -add Ref:prp_coat_front_1_RControlZeroGroup ;select -add Ref:prp_coat_back_0_LControlZeroGroup ;select -add Ref:prp_coat_back_0_MControlZeroGroup ;select -add Ref:prp_coat_back_0_RControlZeroGroup ;delete -cn ;//zero crap out as a hack so that .vaf's will go between primary/secondary scenessetAttr "Ref:prp_coat_back_0_MControlZeroGroup.translateX" 0;setAttr "Ref:prp_coat_back_0_MControlZeroGroup.translateY" 0;setAttr "Ref:prp_coat_back_0_MControlZeroGroup.translateZ" 0;cutKey -cl -t ":" -f ":" -at "tx" -at "ty" -at "tz" Ref:prp_coat_back_0_MControl;setAttr "Ref:prp_coat_back_0_MControl.translateX" 0;setAttr "Ref:prp_coat_back_0_MControl.translateY" 0;setAttr "Ref:prp_coat_back_0_MControl.translateZ" 0;setAttr "Ref:prp_coat_back_0_LControlZeroGroup.translateX" 0;setAttr "Ref:prp_coat_back_0_LControlZeroGroup.translateY" 0;setAttr "Ref:prp_coat_back_0_LControlZeroGroup.translateZ" 0;cutKey -cl -t ":" -f ":" -at "tx" -at "ty" -at "tz" Ref:prp_coat_back_0_LControl;setAttr "Ref:prp_coat_back_0_LControl.translateX" 0;setAttr "Ref:prp_coat_back_0_LControl.translateY" 0;setAttr "Ref:prp_coat_back_0_LControl.translateZ" 0;setAttr "Ref:prp_coat_back_0_RControlZeroGroup.translateX" 0;setAttr "Ref:prp_coat_back_0_RControlZeroGroup.translateY" 0;setAttr "Ref:prp_coat_back_0_RControlZeroGroup.translateZ" 0;cutKey -cl -t ":" -f ":" -at "tx" -at "ty" -at "tz" Ref:prp_coat_back_0_RControl;setAttr "Ref:prp_coat_back_0_RControl.translateX" 0;setAttr "Ref:prp_coat_back_0_RControl.translateY" 0;setAttr "Ref:prp_coat_back_0_RControl.translateZ" 0;}